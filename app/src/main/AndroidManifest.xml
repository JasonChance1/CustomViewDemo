<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CustomView"
        tools:targetApi="31">
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.activities.BasicActivity" />
        <activity android:name=".ui.activities.PieActivity" />
        <activity android:name=".ui.activities.OtherActivity" />
        <activity android:name=".ui.activities.ClockActivity" />
        <activity android:name=".ui.activities.ReverseClockActivity" />
        <activity android:name=".ui.activities.LuckyActivity" />
        <activity android:name=".ui.activities.BitmapActivity" />
        <activity android:name=".ui.activities.LeafLoadingActivity" />
        <activity android:name=".ui.activities.BezierActivity" />
        <activity android:name=".ui.activities.CubicBezierActivity" />
        <activity android:name=".ui.activities.BezierAnimActivity" />
        <activity android:name=".ui.activities.SearchViewActivity" />
        <activity android:name=".ui.activities.GomokuActivity" />
        <activity android:name=".ui.activities.CubeAlbumActivity" />
    </application>

</manifest>